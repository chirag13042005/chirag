import numpy as np
np.linspace(2.0,3.0,num=5)

#OUTPUT
array([2.  , 2.25, 2.5 , 2.75, 3.  ])

import numpy as np
np.linspace(2.0,3.0,num=5,endpoint=False)

#OUTPUT
array([2. , 2.2, 2.4, 2.6, 2.8])

import numpy as np
np.linspace(2.0,3.0,num=5,retstep=True)

#OUTPUT  
(array([2.  , 2.25, 2.5 , 2.75, 3.  ]), 0.25)

